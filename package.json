{
  "name": "raml2html",
  "description": "RAML to HTML documentation generator",
  "version": "4.0.5",
  "author": {
    "name": "Kevin Renskers",
    "email": "info@mixedcase.nl"
  },
  "bugs": {
    "url": "https://github.com/raml2html/raml2html/issues"
  },
  "dependencies": {
    "commander": "2.9.x",
    "marked": "0.3.x",
    "minimize": "2.0.x",
    "nunjucks": "2.5.x",
    "nunjucks-markdown": "2.0.x",
    "raml2obj": "4.0.x"
  },
  "devDependencies": {
    "eslint": "3.11.x",
    "eslint-config-loopwerk": "6.0.x",
    "glob": "7.1.1"
  },
  "homepage": "https://github.com/raml2html/raml2html",
  "keywords": [
    "RAML"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/raml2html/raml2html.git"
  },
  "preferGlobal": true,
  "scripts": {
    "postinstall": "npm run updatesubmodule",
    "updatesubmodule": "git submodule update --recursive --remote",
    "examples": "node examples/render-all-examples.js",
    "lint": "eslint .",
    "lintfix": "eslint . --fix"
  },
  "bin": {
    "raml2html": "./bin/raml2html"
  },
  "files": [
    "index.js",
    "bin/raml2html",
    "lib"
  ],
  "engines": {
    "node": ">=4"
  }
}
